{
  "name": "@novasamatech/product-sdk",
  "type": "module",
  "version": "0.5.0-10",
  "description": "Polkadot product SDK: integrate and run your product inside Polkadot browser.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/novasamatech/spektr-sdk.git"
  },
  "keywords": [
    "polkadot",
    "spektr",
    "dapp"
  ],
  "main": "dist/index.js",
